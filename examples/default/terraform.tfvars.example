# Example variable values for the AI/ML Landing Zone default deployment

# Basic Configuration
location    = "East US 2"
name_prefix = "aiml"

# Network Configuration
hub_vnet_address_space    = "10.10.0.0/24"
ai_lz_vnet_address_space = "192.168.0.0/23"

# Telemetry (optional)
enable_telemetry = true

# Tags (optional)
tags = {
  Environment = "dev"
  Project     = "ai-ml-landing-zone"
  ManagedBy   = "terraform"
  CostCenter  = "IT"
}

# This simplified example creates AI/ML infrastructure directly
# without complex AVM module configurations