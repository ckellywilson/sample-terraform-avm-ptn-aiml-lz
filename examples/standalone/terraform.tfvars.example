# Example variable values for the AI/ML Landing Zone standalone deployment

# Basic Configuration
location    = "East US 2"
name_prefix = "aiml"

# Network Configuration - Larger address space for standalone deployment
ai_lz_vnet_address_space = "192.168.0.0/22"

# Telemetry (optional)
enable_telemetry = true

# Tags (optional)
tags = {
  Environment = "dev"
  Project     = "ai-ml-landing-zone"
  ManagedBy   = "terraform"
  Pattern     = "standalone"
  CostCenter  = "IT"
}